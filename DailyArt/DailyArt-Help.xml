        <command:details>
            <command:name>Show-DailyArt</command:name>
            <command:verb>Show</command:verb>
            <command:noun>DailyArt</command:noun>
            <maml:description>
                <maml:para>Check for and shows a daily art feed in the console.</maml:para>
            </maml:description>
        </command:details>
        <maml:description>
            <maml:para>Display a daily art inside the console using Out-ConsolePicture. The pictures are pulled from a reddit subreddit as a back end feed.

The cmdlet only considers posts that have a direct link to an image. If the image is via a media preview or is a text post it will be ignored.</maml:para>
        </maml:description>
        <command:syntax>
            <command:SyntaxItem>
                <maml:name>Show-DailyArt</maml:name>
                <command:parameter position="named" pipelineInput="false" aliases="" required="false" variableLength="" globbing="false">
                    <maml:name>Refresh</maml:name>
                    <maml:Description>
                        <maml:para>Will ignore the cached picture and download a new daily image.</maml:para>
                    </maml:Description>
                    <command:parameterValue variableLength="False" Required="True">SwitchParameter</command:parameterValue>
                    <dev:type>
                        <maml:name>SwitchParameter</maml:name>
                        <maml:uri />
                    </dev:type>
                </command:parameter>
                <command:parameter position="named" pipelineInput="false" aliases="" required="false" variableLength="" globbing="false">
                    <maml:name>IncludeDescription</maml:name>
                    <maml:Description>
                        <maml:para>Include a short line with the title of the source post.</maml:para>
                    </maml:Description>
                    <command:parameterValue variableLength="False" Required="True">SwitchParameter</command:parameterValue>
                    <dev:type>
                        <maml:name>SwitchParameter</maml:name>
                        <maml:uri />
                    </dev:type>
                </command:parameter>
            </command:SyntaxItem>
        </command:syntax>
        <command:Parameters>
            <command:parameter position="named" pipelineInput="false" aliases="" required="false" variableLength="" globbing="false">
                <maml:name>Refresh</maml:name>
                <maml:Description>
                    <maml:para>Will ignore the cached picture and download a new daily image.</maml:para>
                </maml:Description>
                <command:parameterValue variableLength="False" Required="True">SwitchParameter</command:parameterValue>
                <dev:type>
                    <maml:name>SwitchParameter</maml:name>
                    <maml:uri />
                </dev:type>
            </command:parameter>
            <command:parameter position="named" pipelineInput="false" aliases="" required="false" variableLength="" globbing="false">
                <maml:name>IncludeDescription</maml:name>
                <maml:Description>
                    <maml:para>Include a short line with the title of the source post.</maml:para>
                </maml:Description>
                <command:parameterValue variableLength="False" Required="True">SwitchParameter</command:parameterValue>
                <dev:type>
                    <maml:name>SwitchParameter</maml:name>
                    <maml:uri />
                </dev:type>
            </command:parameter>
        </command:Parameters>
        <command:inputTypes>
            <command:inputType>
                <dev:type>
                    <maml:name />
                </dev:type>
            </command:inputType>
            <maml:description>
                <maml:para>InputType description</maml:para>
            </maml:description>
        </command:inputTypes>
        <command:returnValues>
            <command:returnValue>
                <dev:type>
                    <maml:name />
                </dev:type>
            </command:returnValue>
            <maml:description>
                <maml:para>Output Type description</maml:para>
            </maml:description>
        </command:returnValues>
        <maml:alertSet>
            <maml:alert>
                <maml:para>The cmdlet requires the module OutConsolePicture to work.</maml:para>
            </maml:alert>
        </maml:alertSet>
        <command:examples>
            <command:example>
                <maml:title>-------------------------- EXAMPLE 1 --------------------------</maml:title>
                <dev:code>Show-DailyArt</dev:code>
                <dev:remarks>
                    <maml:para>Displays either the cached or a new picture.</maml:para>
                    <maml:para />
                    <maml:para />
                    <maml:para />
                    <maml:para />
                </dev:remarks>
            </command:example>
